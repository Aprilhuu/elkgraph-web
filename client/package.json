{
  "name": "elkgraph",
  "version": "0.1.0",
  "description": "A web page with a text editor for ELK Graph and a synchronized graphical view",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "monaco-languageclient": "0.0.1-alpha.6",
    "reconnecting-websocket": "^3.0.5",
    "sprotty": "file:../../sprotty/repos/client",
    "vscode-ws-jsonrpc": "0.0.1-alpha.4"
  },
  "devDependencies": {
    "@types/node": "^8.0.2",
    "copy-webpack-plugin": "^4.0.1",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "tsc-watch": "^1.0.7",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1"
  },
  "scripts": {
    "compile": "./node_modules/.bin/tsc",
    "clean": "./node_modules/.bin/rimraf lib app/bootstrap app/sprotty app/vs app/bundle.*",
